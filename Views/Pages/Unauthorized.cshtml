@model Feature
<!-- «Start| UnauthorizedAccess» -->
<form data-module="UnauthorizedAccess" method="post" action="@Url.Current()" class="error unauthorized">
   @Html.AntiForgeryToken()
   @Html.StartupActionsJson()
   <div class="view-body">
      <div class="form-group row">
            <div class="group-control">
                <h2>Access denied 🔒</h2>
                <p>
                    You do not appear to have access to Upcoming events.
                </p>
                <p>
                    You are currently logged in as @User?.GetEmail().
                </p><h5 style="margin-top: 30px;">
                    Suggestions
                </h5><hr>
                <ul><li><a href="/login?returnurl=@Url.Current()">Log in</a> as a different user.</li><li>Request to be granted permission for this page.</li></ul><p></p>
            </div>
         </div>
      </div>
   </form>
<!-- «End| UnauthorizedAccess» -->
